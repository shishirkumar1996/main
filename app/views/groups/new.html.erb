<%= render :partial=> 'layouts/header' %>

<div class = "row">
<div class = "col-md-4 col-md-offset-4 new_group_form">
<h1>New group</h1>

<%= form_for(@group) do |f| %>
	<%= render 'common/error_messages', object: f.object %>
	
	<%= f.label :title %>
	<%= f.text_field :name %>
	
	<%= f.submit %>
<% end %>
</div>
</div>
