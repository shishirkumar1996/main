<p id="notice"><%= notice %></p>

<h1>Grouparticlereplies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grouparticlereplies.each do |grouparticlereply| %>
      <tr>
        <td><%= link_to 'Show', grouparticlereply %></td>
        <td><%= link_to 'Edit', edit_grouparticlereply_path(grouparticlereply) %></td>
        <td><%= link_to 'Destroy', grouparticlereply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

