<p id="notice"><%= notice %></p>

<h1>Articlereplies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articlereplies.each do |articlereply| %>
      <tr>
        <td><%= link_to 'Show', articlereply %></td>
        <td><%= link_to 'Edit', edit_articlereply_path(articlereply) %></td>
        <td><%= link_to 'Destroy', articlereply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Articlereply', new_articlereply_path %>
